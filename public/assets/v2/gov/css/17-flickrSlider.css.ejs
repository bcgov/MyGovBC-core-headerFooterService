.bcgovHF .flickr_link {
  font-weight: bold;
  display: block;
  background-image: url("<%=unippearHost%><%=thisFileUrlPath%>/../img/arrow_blue.png");
  background-repeat: no-repeat;
  padding-left: 15px;
  margin-left: 10px;
  margin-top: 10px;
}
.bcgovHF #main-content .flickr_link {
  width: 96%;
}
.bcgovHF .rightColumnBox .flickr_link {
  width: 90%;
}
.bcgovHF .flickr_wysiwyg_embed iframe {
  width: 100%;
}
@media only screen and (min-width: 992px) {
  .bcgovHF .rightColumnBox .flickr_wysiwyg_embed iframe {
    min-height: 180px;
  }
}
@media only screen and (max-width: 991px) and (min-width: 445px) {
  .bcgovHF .rightColumnBox .flickr_wysiwyg_embed iframe {
    min-height: 280px;
  }
}
@media only screen and (max-width: 444px) and (min-width: 315px) {
  .bcgovHF .rightColumnBox .flickr_wysiwyg_embed iframe {
    min-height: 280px;
  }
}
@media only screen and (max-width: 315px) {
  .bcgovHF .rightColumnBox .flickr_wysiwyg_embed iframe {
    min-height: 280px;
  }
}
.bcgovHF #main-content .flickr_wysiwyg_embed iframe {
  min-height: 280px;
}
