}

.bcgovHF .featureBoxes-3col {
	margin-bottom: 15px;
}

.bcgovHF .featureBoxes-3col>.featureBox, .tiles-row>.tile {
	background-color: #fff; /* for IE */
	background-color: rgba(255, 255, 255, 0.80);
