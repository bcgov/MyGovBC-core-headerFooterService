@font-face {
	font-family: 'Myriad-Pro';
	src: url('<%=unippearHost%><%=thisFileUrlPath%>/../fonts/MyriadWebPro.ttf');
}

html, body {
	height: 100%;
}

body {
	font-family: Myriad-Pro, Calibri, Arial, sans serif;
	background-color: #f1f1f1;
	color: #494949;
	font-size: 13px;
}

.bcgovHF a {
	color: #1a5a96;
}

.bcgovHF h1, h2, h3, h4, h5 {
	margin-top: 0;
	color: #494949;
}

.bcgovHF h1 {
	font-size: 24px;
	font-weight: 600;
}

.bcgovHF h2 {
	font-size: 21px;
}

.bcgovHF h3 {
	font-size: 18px;
}

.bcgovHF h4 {
	font-size: 15px;
}

.bcgovHF [class*='col-'] {
	padding-left: 0;
	padding-right: 0;
}

.bcgovHF table {
	margin: 10px 0;
}

.bcgovHF td {
	padding: 4px;
}

.bcgovHF th {
	padding: 4px;
	text-align: center;
}

.bcgovHF .row {
	margin: 0;
}

.bcgovHF .table-responsive {
	border: none;
}

#header {
	width: 100%;
	z-index: 2;
}

.bcgovHF #header-main {
	background-color: #003366;
	margin-bottom: 0;
	border: none;
	/* 	border-bottom: 2px solid #fcba19; */
	/* 	padding-bottom: 5px; */
}

#header .header-main-left, #header .header-main-right {
	padding: 0;
}

.bcgovHF #logo {
	float: left;
}

#header .logo {
	display: block;
}

.bcgovHF #access {
	display: inline;
	height: 0;
	position: absolute;
	width: 0;
	z-index: 9;
}

.bcgovHF #access ul {
	padding-left: 10px;
	list-style: none;
}

.bcgovHF #access li a {
	margin: 10px 0 0 -5000px;
	width: 90px;
	padding: 5px;
	position: absolute;
	top: 0;
	color: #fcba19;
}

.bcgovHF #access li a:focus {
	margin-left: 0;
}

#header .qa-banner {
	background-color: black;
}

#header .qa-banner .container {
	text-align: center;
	max-width: 1140px;
	padding: 15px;
}

#header .qa-banner .container .qa-banner-text {
	color: #fff;
	font-size: 18px;
}

#header .menu-button {
	background-color: #38598a;
	margin-top: 14px;
	padding: 4px 0 3px;
	text-align: center;
	width: 155px;
	color: #fff;
	border: 1px solid white;
	cursor: pointer;
}

#header .menu-button:hover {
	background-color: #5475A7;
}

#header .menu-button span {
	margin: 0;
	font-weight: 400;
	cursor: pointer;
}

#header .menu-button img {
	margin-right: 6px;
	margin-bottom: 2px;
}

#header .search {
	height: 25px;
	/* 	width: 100%; */
	margin-top: 20px;
	margin-left: 10px;
	position: relative;
}

#header .search input {
	border: 0 none;
	color: #494949;
	height: 25px;
	outline: 0 none;
	padding: 3px 10px;
	width: 100%;
}

#header .search .search-trigger {
	margin-top: -25px;
	cursor: pointer;
	height: 25px;
	position: relative;
	right: 6px;
	float: right;
	width: 25px;
	background-image: url("<%=unippearHost%><%=thisFileUrlPath%>/../img/search-ico.png");
	background-position: 98% center;
	background-repeat: no-repeat;
}

.bcgovHF #header-links {
	margin-top: 10px;
}

.bcgovHF #header-links a {
	color: #fff;
}

@media ( min-width : 768px) {
	#header-links>ul li>a {
		padding: 0;
	}
}

.bcgovHF #header-main-row2 {
	padding-bottom: 10px;
}

.bcgovHF .navigationRibbon {
	-webkit-box-shadow: 0px 3px 3px 1px rgba(51, 51, 51, 0.5);
	-moz-box-shadow: 0px 3px 3px 1px rgba(51, 51, 51, 0.5);
	box-shadow: 0px 3px 3px 1px rgba(51, 51, 51, 0.5);
	border-top: 2px solid #fcba19;
}

.bcgovHF .navigationRibbon a {
	color: #fff;
	display: table-cell;
	text-align: center;
	vertical-align: middle;
	padding: 10px 15px;
	position: relative;
}

.bcgovHF .navigationRibbon a+a:after {
	content: "";
	background: #748bad;
	position: absolute;
	bottom: 25%;
	left: 0;
	height: 50%;
	width: 1px;
}

.bcgovHF .navigationRibbon a.current {
	text-decoration: underline;
	font-weight: bold;
}

.bcgovHF .navigationRibbon .level2Navigation a.current {
	background-image: url("<%=unippearHost%><%=thisFileUrlPath%>../images/nav-ribbon-arrow.png");
	background-position: center bottom;
	background-repeat: no-repeat;
}

.bcgovHF .navigationRibbon .level2Navigation {
	background-color: #38598a;
}

.bcgovHF .navigationRibbon .level3Navigation {
	background-color: #5475a7;
}

.bcgovHF .header-main-right {
	margin-top: 5px;
}

.bcgovHF #main-content-anchor {
	display: block;
	visibility: hidden;
}

.bcgovHF #main-content .table-responsive {
	overflow-x: visible;
	overflow-y: visible;
}

.bcgovHF ul.inline {
	list-style-type: none;
	margin: 0;
	overflow: hidden;
	padding: 0;
}

.bcgovHF ul.inline li {
	float: left;
	padding: 2px 10px;
	position: relative;
}

@media ( min-width : 768px) {
	ul.inline li+li {
		border-left: 1px solid #4b5e73;
	}
}

.bcgovHF ul.inline li a:link, nav ul.inline li a:visited {
	color: white;
	display: block;
	text-align: center;
}

.bcgovHF .iframe-video-embed, .iframe-video-embed>iframe {
	max-width: 100% !important;
}

.bcgovHF .iframe-map-embed, .iframe-map-embed>iframe {
	max-width: 100% !important;
}

#footer {
	background-color: transparent;
	border-top: 2px solid #fcba19;
	position: relative;
}

#footer.expanded {
	z-index: 1040;
}

#footer a, #footer h2 {
	color: #fff;
}

#footer h2 {
	font-size: 18px;
	margin-bottom: 10px;
	margin-top: 22px;
	padding-right: 20px;
}

#footer hr {
	background: none repeat scroll 0 0 #4b5e73;
	border: 0 none;
	height: 1px;
	margin: 10px 0;
}

#footer #footerWrapper {
	/* 	position: absolute; */
	width: 100%;
	bottom: 0;
}

.bcgovHF #footerAdminSection ul {
	clear: both;
	padding: 10px 0;
}

.bcgovHF #footerAdminLinks {
	clear: both;
}

@media ( min-width : 768px) {
	#footerMediaLinks ul {
		padding-top: 22px;
	}
}

.bcgovHF #footerMediaLinks h2 {
	display: inline-block;
}

.bcgovHF #footerFeedback {
	position: relative;
	height: 50px;
}

.bcgovHF #footerToggle {
	background-color: #003366;
	text-align: center;
}

.bcgovHF #footerCollapsible {
	display: none;
	background-color: #003366;
	border: 1px solid transparent;
}

.bcgovHF #footerAdminSection {
	background-color: #003366;
}

.bcgovHF #feedbackWrapper {
	position: absolute;
	bottom: 0;
	right: 0;
	color: #fff;
}

.bcgovHF #feedbackToggle {
	background-color: #38598a;
	cursor: pointer;
	width: 220px;
	height: 40px;
	padding: 10px 0;
	text-align: center;
}

.bcgovHF #feedbackForm {
	display: none;
	width: 220px;
	height: 300px;
	background-color: #38598a;
}

.bcgovHF .template {
	min-height: 100%;
	padding-bottom: 80px;
	background-color: #fff;
	font-size: 14px;
}

.bcgovHF #homeTemplate {
	background-color: transparent;
	margin-top: -20px;
}

.bcgovHF #errorPage {
	padding: 10px;
}

@media ( min-width : 768px) {
	#homeTemplate {
		padding-top: 200px;
	}
	#errorPage {
		padding-top: 150px;
	}
}

.bcgovHF #themeTemplate {
	background-position: right 150px;
	background-repeat: no-repeat;
	background-color: transparent;
}

.bcgovHF #errorPage {
	background-position: right 150px;
	background-repeat: no-repeat;
	background-color: transparent;
}

@media ( min-width : 992px) {
	.contentPageRightColumn, .homePageRightColumn {
		padding-left: 15px;
	}
}

@media ( min-width : 768px) {
	.contentPageLeftNavigation {
		padding-right: 15px;
	}
}

.bcgovHF .contentPageLeftNavigation ul {
	list-style: none;
	padding-left: 0;
}

.bcgovHF .contentPageLeftNavigation ul.leftNav {
	border: 1px solid #e1e1e1;
	padding-bottom: 10px;
	padding-right: 10px;
	padding-left: 10px;
	width: 100%;
}

.bcgovHF .contentPageLeftNavigation ul li {
	width: 100%;
}

.bcgovHF .contentPageLeftNavigation>div {
	padding-left: 10px;
}

.bcgovHF .contentPageLeftNavigation ul li {
	list-style: none;
	background-position: 0 4px;
	background-repeat: no-repeat;
	line-height: 14px;
	margin-top: 10px;
	padding-left: 10px;
	padding-right: 10px;
}

.bcgovHF ul.leftNav>li:first-child {
	margin-top: 0;
}

.bcgovHF .contentPageLeftNavigation div>ul {
	padding-bottom: 26px;
	padding-top: 10px;
	top: 10px;
}

.bcgovHF .contentPageLeftNavigation ul li.closed {
	background-image: url("<%=unippearHost%><%=thisFileUrlPath%>../images/sidecolumnnav_closed.gif");
	background-position: 0 2px;
}

.bcgovHF .contentPageLeftNavigation ul li ul {
	display: none;
}

.bcgovHF .contentPageLeftNavigation ul li.open ul {
	display: block;
}

.bcgovHF .contentPageLeftNavigation ul li.solo {
	background-image: none;
}

.bcgovHF .contentPageLeftNavigation ul li.open {
	background-image: url("<%=unippearHost%><%=thisFileUrlPath%>../images/sidecolumnnav_open.gif");
	background-position: 0 2px;
}

.bcgovHF .contentPageLeftNavigation ul li a {
	text-decoration: none;
	display: block;
	padding: 2px 0 0 4px;
}

.bcgovHF #topicLeftNav, #topicLeftNav .navbar-collapse {
	padding: 0;
	border: none;
}

.bcgovHF #topicLeftNav li.open a {
	background-color: inherit;
}

.bcgovHF .contentPageLeftNavigation ul li a:hover {
	text-decoration: underline;
}

.bcgovHF .contentPageLeftNavigation ul li.active>div.leftNav-item-wrapper a {
	font-weight: bold;
	text-decoration: underline;
}

.bcgovHF .contentPageLeftNavigation ul li.current>div.leftNav-item-wrapper  a {
	font-weight: bold;
	text-decoration: underline;
}

.bcgovHF .blue-heading-bar {
	background-color: #003366; /* for IE */
	background-color: rgba(0, 51, 102, 0.80);
	color: #fff;
