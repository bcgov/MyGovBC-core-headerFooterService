@font-face {
  font-family: 'Myriad-Pro';
  src: url("<%=unippearHost%><%=thisFileUrlPath%>/../fonts/MyriadWebPro.ttf");
}
html,
body {
  height: 100%;
}
body {
  font-family: Myriad-Pro, Calibri, Arial, 'sans serif';
  background-color: #f1f1f1;
  color: #494949;
  font-size: 16px;
}
.bcgovHF a {
  color: #1a5a96;
}
.bcgovHF h1,
.bcgovHF h2,
.bcgovHF h3,
.bcgovHF h4,
.bcgovHF h5 {
  margin-top: 0;
  color: #494949;
}
.bcgovHF h1 {
  font-size: 24px;
  font-weight: 600;
}
.bcgovHF h2 {
  font-size: 22px;
}
.bcgovHF h3 {
  font-size: 20px;
}
.bcgovHF h4 {
  font-size: 18px;
}
.bcgovHF .row {
  margin: 0;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
  .bcgovHF .iframe-events-embed .embed-responsive,
  .bcgovHF .iframe-embed .embed-responsive {
    -webkit-overflow-scrolling: touch !important;
    overflow-y: scroll !important;
  }
}
.bcgovHF ul.inline {
  list-style-type: none;
  margin: 0;
  overflow: hidden;
  padding: 0;
}
.bcgovHF ul.inline li {
  float: left;
  padding: 2px 10px;
  position: relative;
}
.bcgovHF ul.inline li a:link,
.bcgovHF ul.inline li a:visited {
  color: #fff;
  display: block;
  text-align: center;
}
@media (min-width: 768px) {
  .bcgovHF ul.inline li+li {
    border-left: 1px solid #4b5e73;
  }
}
.bcgovHF .iframe-video-embed,
.bcgovHF .iframe-video-embed>iframe {
  max-width: 100%;
}
.bcgovHF .iframe-map-embed,
.bcgovHF .iframe-map-embed>iframe {
  max-width: 100%;
}
.bcgovHF .iframe-visualization-embed,
.bcgovHF .iframe-visualization-embed>iframe {
  max-width: 100%;
}
.bcgovHF .alert-success {
  color: #2d4821;
}
.bcgovHF .alert-warning {
  color: #6c4a00;
  background-color: #f9f1c6;
}
.bcgovHF .alert-danger {
  color: #a12622;
}
